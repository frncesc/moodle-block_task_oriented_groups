         </KEYS>
      </TABLE>
      <TABLE NAME="btog_personality" COMMENT="Stores the personality of an user.">
         <FIELDS>
            <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" NEXT="userid" />
            <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="id"  NEXT="type" />
            <FIELD NAME="type" TYPE="char" LENGTH="4" NOTNULL="true" SEQUENCE="false"  PREVIOUS="id"  NEXT="gender" />
            <FIELD NAME="gender" TYPE="int" LENGTH="1" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="type"  NEXT="judgment" />
            <FIELD NAME="judgment" TYPE="number" LENGTH="15" NOTNULL="true" SEQUENCE="false" DECIMALS="14" PREVIOUS="type"  NEXT="attitude" />
            <FIELD NAME="attitude" TYPE="number" LENGTH="15" NOTNULL="true" SEQUENCE="false" DECIMALS="14" PREVIOUS="judgment"  NEXT="perception"/>
            <FIELD NAME="perception" TYPE="number" LENGTH="15" NOTNULL="true" SEQUENCE="false" DECIMALS="14" PREVIOUS="attitude"  NEXT="extrovert"/>
            <FIELD NAME="extrovert" TYPE="number" LENGTH="15" NOTNULL="true" SEQUENCE="false" DECIMALS="14" PREVIOUS="perception" />
         </FIELDS>
         <KEYS>
            <KEY NAME="primary" TYPE="primary" FIELDS="id" />
         </KEYS>
      </TABLE>
      <TABLE NAME="btog_competences" COMMENT="Stores the competences of an user.">
         <FIELDS>
            <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" NEXT="userid" />
            <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="id"  NEXT="verbal" />
            <FIELD NAME="verbal" TYPE="number" LENGTH="15" NOTNULL="true" SEQUENCE="false" DECIMALS="14" PREVIOUS="userid"  NEXT="logic_mathematics" />
            <FIELD NAME="logic_mathematics" TYPE="number" LENGTH="15" NOTNULL="true" SEQUENCE="false" DECIMALS="14" PREVIOUS="verbal"  NEXT="visual_spatial"/>
            <FIELD NAME="visual_spatial" TYPE="number" LENGTH="15" NOTNULL="true" SEQUENCE="false" DECIMALS="14" PREVIOUS="logic_mathematics"  NEXT="kinestesica_corporal"/>
            <FIELD NAME="kinestesica_corporal" TYPE="number" LENGTH="15" NOTNULL="true" SEQUENCE="false" DECIMALS="14" PREVIOUS="visual_spatial" NEXT="musical_rhythmic"/>
            <FIELD NAME="musical_rhythmic" TYPE="number" LENGTH="15" NOTNULL="true" SEQUENCE="false" DECIMALS="14" PREVIOUS="kinestesica_corporal"  NEXT="intrapersonal"/>
            <FIELD NAME="intrapersonal" TYPE="number" LENGTH="15" NOTNULL="true" SEQUENCE="false" DECIMALS="14" PREVIOUS="musical_rhythmic"  NEXT="interpersonal"/>
            <FIELD NAME="interpersonal" TYPE="number" LENGTH="15" NOTNULL="true" SEQUENCE="false" DECIMALS="14" PREVIOUS="intrapersonal" NEXT="naturalist_environmental"/>
            <FIELD NAME="naturalist_environmental" TYPE="number" LENGTH="15" NOTNULL="true" SEQUENCE="false" DECIMALS="14" PREVIOUS="interpersonal" />
         </FIELDS>
         <KEYS>
            <KEY NAME="primary" TYPE="primary" FIELDS="id" />
         </KEYS>
      </TABLE>
   </TABLES>
